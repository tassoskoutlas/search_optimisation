# Exclude IDE specific paths
.project
.buildpath
.settings

# Ignore paths that contain user-generated content.
sites/*/files
sites/*/private

sites/all/modules/contrib
!sites/all/modules/custom
sites/all/libraries
sites/all/themes

!sites/default/settings.php
sites/default/settings.*.php
